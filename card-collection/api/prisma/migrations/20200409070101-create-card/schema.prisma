datasource DS {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Card {
  id    Int     @id @default(autoincrement())
  name String
  brand String
  year String
  series String
  number String?
  grade String?
  image String?
  postedAt DateTime?
}
